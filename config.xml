<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<widget id="com.ionicframework.foodscanner796316" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>Gronkoll</name>
  <description>
        An Ionic Framework and Cordova project.
    </description>
  <author email="johan@kasperi.se" href="http://kasperi.se/">
      Johan Kasperi och Daniel Lindstr√∂m
    </author>
  <content src="index.html"/>
  <access origin="*"/>
  <preference name="webviewbounce" value="false"/>
  <preference name="UIWebViewBounce" value="false"/>
  <preference name="DisallowOverscroll" value="true"/>
  <preference name="orientation" value="portrait" />
  <preference name="BackupWebStorage" value="none"/>
  <feature name="StatusBar">
    <param name="ios-package" value="CDVStatusBar" onload="true"/>
  </feature>
  <platform name="ios">
      <!-- iOS 8.0+ -->
      <!-- iPhone 6 Plus  -->
      <icon src="res/ios/apple-touch-icon-180x180.png" width="180" height="180" />
      <!-- iOS 7.0+ -->
      <!-- iPhone / iPod Touch  -->
      <icon src="res/ios/apple-touch-icon-60x60.png" width="60" height="60" />
      <icon src="res/ios/apple-touch-icon-120x120.png" width="120" height="120" />
      <!-- iPad -->
      <icon src="res/ios/apple-touch-icon-76x76.png" width="76" height="76" />
      <icon src="res/ios/apple-touch-icon-152x152.png" width="152" height="152" />
      <!-- iOS 6.1 -->
      <!-- Spotlight Icon -->
      <icon src="res/ios/apple-touch-icon-40x40.png" width="40" height="40" />
      <icon src="res/ios/apple-touch-icon-80x80.png" width="80" height="80" />
      <!-- iPhone / iPod Touch -->
      <icon src="res/ios/apple-touch-icon-57x57.png" width="57" height="57" />
      <icon src="res/ios/apple-touch-icon-114x114.png" width="114" height="114" />
      <!-- iPad -->
      <icon src="res/ios/apple-touch-icon-72x72.png" width="72" height="72" />
      <icon src="res/ios/apple-touch-icon-144x144.png" width="144" height="144" />
      <!-- iPhone Spotlight and Settings Icon -->
      <icon src="res/ios/apple-touch-icon-29x29.png" width="29" height="29" />
      <icon src="res/ios/apple-touch-icon-58x58.png" width="58" height="58" />
      <!-- iPad Spotlight and Settings Icon -->
      <icon src="res/ios/apple-touch-icon-50x50.png" width="50" height="50" />
      <icon src="res/ios/apple-touch-icon-100x100.png" width="100" height="100" />
      <splash src="res/screen/ios/screen-iphone-portrait-320x480.png" width="320" height="480"/>
      <splash src="res/screen/ios/screen-iphone-portrait-640x960.png" width="640" height="960"/>
      <splash src="res/screen/ios/screen-ipad-portrait-768x1024.png" width="768" height="1024"/>
      <splash src="res/screen/ios/screen-ipad-portrait-1536x2048.png" width="1536" height="2048"/>
      <splash src="res/screen/ios/screen-iphone-portrait-640x1136.png" width="640" height="1136"/>
      <splash src="res/screen/ios/screen-iphone-portrait-750x1334.png" width="750" height="1334"/>
      <splash src="res/screen/ios/screen-iphone-portrait-1242x2208.png" width="1242" height="2208"/>
  </platform>
  <platform name="android">
    <icon src="res/android/ldpi.png" density="ldpi" />
    <icon src="res/android/mdpi.png" density="mdpi" />
    <icon src="res/android/hdpi.png" density="hdpi" />
    <icon src="res/android/xhdpi.png" density="xhdpi" />
        <!-- Splash screen -->
    <splash src="res/screen/android/splash-port-hdpi.png" density="port-hdpi"/>
    <splash src="res/screen/android/splash-port-ldpi.png" density="port-ldpi"/>
    <splash src="res/screen/android/splash-port-mdpi.png" density="port-mdpi"/>
    <splash src="res/screen/android/splash-port-xhdpi.png" density="port-xhdpi"/>
  </platform>
</widget>